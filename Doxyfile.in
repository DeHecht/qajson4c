PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@
FULL_PATH_NAMES        = NO
INPUT                  = README.md \
                         @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
FILE_PATTERNS          = *.h
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = README.md
EXTRACT_STATIC            = YES
OUTPUT_DIRECTORY       = doc
OPTIMIZE_OUTPUT_FOR_C  = YES
INLINE_SIMPLE_STRUCTS  = YES

SHOW_NAMESPACES        = NO
CLASS_GRAPH            = NO
INCLUDE_GRAPH          = NO
